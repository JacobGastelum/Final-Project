<body id="greenBackground">
  <h3>
    <a href="/users">Booking List</a>
  </h3>
  <h1>Create New Booking</h1>
  <table>
    <tbody>
      <tr>
        <td>Contact Name</td>
        <td>
          <input type="text" name="name" />
        </td>
      </tr>
      <td>Organization</td>
      <td>
        <input type="text" name="organization" />
      </td>
    </tr>
      <tr>
        <td>Contact Phone Number</td>
        <td>
          <input type="tel" name="phoneNumber" />
        </td>
      </tr>
      <tr>
      <tr>
        <td>Payment</td>
        <td>
          <input type="number" name="payment" />
        </td>
      </tr>
      <tr>
        <td colspan="2">
          <button id="createButton"><a href="https://shielded-journey-79042.herokuapp.com/users">Create Booking</a></button>
        </td>
      </tr>
    </tbody>
  </table>
